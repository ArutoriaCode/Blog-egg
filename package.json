{
  "name": "blog-egg",
  "version": "1.0.0",
  "description": "About 仿照 https://www.yevpt.com/ 的一个博客后端，基于egg.js",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "egg": "^2.15.1",
    "egg-jwt": "^3.1.7",
    "egg-mysql": "^3.0.0",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^6.0.0",
    "lodash": "^4.17.20",
    "mysql2": "^2.2.5",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev": "npx sequelize db:migrate && egg-bin dev",
    "migrateDB": "npx sequelize db:migrate",
    "start": "egg-scripts start --daemon --title=egg-server-blog-egg",
    "stop": "egg-scripts stop --title=egg-server-blog-egg",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Arutoria",
  "license": "MIT"
}
